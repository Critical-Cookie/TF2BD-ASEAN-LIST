					"cheater"
				]
			},
			"description": "Bot Advertising Link Detection - TF2BD-ASEAN",
			"triggers": {
				"username_text_match": {
					"case_sensitive": false,
					"mode": "starts_with",
					"patterns": [
						"discord.io/",
						"discord.gg/",
						"discord.me/",
						"dsc.gg/",
						"disc.gg/",
						"youtube.com/",
						"youtu.be/",
						"shoppy.gg",
						"DISCORD.GG/",
						"lmaobox.net"
					]
				}
			}
		},
