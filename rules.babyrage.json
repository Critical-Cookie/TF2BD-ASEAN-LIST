{
	"$schema": "https://raw.githubusercontent.com/PazerOP/tf2_bot_detector/master/schemas/v3/rules.schema.json",
	"file_info": {
		"authors": [
			"Critical-Cookie"
		],
		"description": "This is a WIP Ruleset for a BOT ONLY list for bots found prominent in S.E.A servers such as Babyrage, Enigmaballz and EAC |Texashook",
		"title": "Babyrage and Friends 2 - Ruleset Boogaloo!",
		"update_url": "https://raw.githubusercontent.com/Critical-Cookie/babyrage-and-friends/main/rules.babyrage.json"
	},
	"rules": [
		{
			"actions": {
				"mark": [
					"cheater"
				]
			},
			"description": "Babyrage Steam or ingame name",
			"triggers": {
				"username_text_match": {
					"case_sensitive": false,
					"mode": "contains",
					"patterns": [
						"Babyrage",
						"Serrabella",
						"[ATG]",
						"lickass",
						"discord.io/babyrage",
						"Citrous"
					]
				}
			}
		},
		{
			"actions": {
				"transient_mark": [
					"cheater"
				]
			},
			"description": "discord.io/babyrage regex detection test",
			"triggers": {
				"username_text_match": {
					"case_sensitive": true,
					"mode": "regex",
					"patterns": [
						"(\\([1-9]\\d?\\))?discord\\.io/babyrage \\| yt/l1tty"
					]
				}
			}
		},
		{
